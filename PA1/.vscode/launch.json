    "version": "0.2.0",
    "configurations": [
        {

         "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/sta", // Path to your compiled executable
            "args": ["test/NLDM_lib_max2Inp", "test/cleaned_iscas89_99_circuits/c17.isc", "10", "11"], // Add any arguments you need
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            // "preLaunchTask": "C/C++: g++ build active file",
        }
    ]
}